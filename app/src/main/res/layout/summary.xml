<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Summary"
        android:textColor="@color/colorAccent"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginTop="54dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="101dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shipping Cost"
        android:textSize="17sp"
        android:layout_marginStart="59dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="41dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Insurance Premium"
        android:textColor="@color/colorAccent"
        android:textSize="17sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/premium"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Express/Hold\nShipment Charges"
        android:textColor="@color/colorAccent"
        android:textSize="17sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/express_charges"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="117dp"
        android:layout_height="28dp"
        android:text="Total Cost"
        android:textStyle="bold"
        android:textSize="17sp"
        android:textColor="@color/colorAccent"
        app:layout_constraintBaseline_toBaselineOf="@+id/total_charges"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView5" />

    <TextView
        android:id="@+id/shipping_cost"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:text=""
        android:textSize="17sp"
        android:textColor="@color/colorAccent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/express_charges"
        app:layout_constraintRight_toRightOf="@+id/express_charges"
        android:layout_marginTop="69dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="69dp"
        app:layout_constraintLeft_toLeftOf="@+id/premium"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintVertical_bias="0.04" />

    <TextView
        android:id="@+id/premium"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:text=""
        android:textSize="17sp"
        android:textColor="@color/colorAccent"
        android:layout_marginEnd="6dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="6dp"
        app:layout_constraintBottom_toBottomOf="@+id/express_charges"
        app:layout_constraintRight_toRightOf="@+id/total_charges"
        android:layout_marginTop="44dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="48dp"
        app:layout_constraintTop_toTopOf="@+id/shipping_cost"
        app:layout_constraintLeft_toRightOf="@+id/textView4" />

    <TextView
        android:id="@+id/express_charges"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:text=""
        android:textSize="17sp"
        android:textColor="@color/colorAccent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="6dp"
        app:layout_constraintBottom_toBottomOf="@+id/total_charges"
        android:layout_marginEnd="4dp"
        app:layout_constraintRight_toRightOf="@+id/total_charges"
        android:layout_marginTop="70dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="70dp"
        app:layout_constraintLeft_toRightOf="@+id/textView4" />

    <TextView
        android:id="@+id/total_charges"
        android:layout_width="91dp"
        android:layout_height="27dp"
        android:text=""
        android:textSize="17sp"
        android:textColor="@color/colorAccent"
        android:textStyle="bold"
        android:layout_marginStart="13dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="208dp"
        app:layout_constraintLeft_toRightOf="@+id/textView5" />

</android.support.constraint.ConstraintLayout>
